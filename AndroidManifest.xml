        <activity android:name="com.inz.rogovskycurrentmeter.chart.ChartDemo" 
            android:theme="@android:style/Theme.Holo.Dialog" />
       
      
        
             <activity android:name="com.inz.rogovskycurrentmeter.chart.XYChartBuilder" />
